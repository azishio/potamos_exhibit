{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "standard-with-typescript",
        "airbnb-base",
        "airbnb-typescript/base",
        "prettier"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "project": "./tsconfig.json"
    },
    "rules": {
        "no-console": 0,
        "no-plusplus": 0,
        "yoda": 0,
        "no-continue":0,
        "@typescript-eslint/no-invalid-void-type": 0,
        "no-underscore-dangle": 0,
        "@typescript-eslint/no-non-null-assertion": 0,
        "import/extensions": 0
    }
}
